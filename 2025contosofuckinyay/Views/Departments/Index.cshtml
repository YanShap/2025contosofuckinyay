@model IEnumerable<_2025contosofuckinyay.Models.Department>

@{
	ViewData["Title"] = "Osakonnad";
}
<h1>Osakonnad Index</h1>
<p>
	<a asp-action="Create">Loo uus osakond</a>
</p>
<table class="table">
	<thead>
		<tr>
			<th>@Html.DisplayNameFor(model => model.Name)</th>
			<th>@Html.DisplayNameFor(model => model.Budget)</th>
			<th>@Html.DisplayNameFor(model => model.OpeningDate)</th>
			<th>@Html.DisplayNameFor(model => model.Address)</th>
			<th>@Html.DisplayNameFor(model => model.OpenTime)</th>
			<th>@Html.DisplayNameFor(model => model.Description)</th>
			<th>@Html.DisplayNameFor(model => model.ShortName)</th>
			<th>@Html.DisplayNameFor(model => model.InstructorID)</th>
			<th>@Html.DisplayNameFor(model => model.RowVersion)</th>
			<th>@Html.DisplayNameFor(model => model.Administrator)</th>
			<th>@Html.DisplayNameFor(model => model.Courses)</th>
			<th>Tegevused</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var department in Model)
		{
			<tr>
				<td>@Html.DisplayFor(modelItem => department.Budget)</td>
				<td>@Html.DisplayFor(modelItem => department.OpeningDate)</td>
				<td>@Html.DisplayFor(modelItem => department.Address)</td>
				<td>@Html.DisplayFor(modelItem => department.OpenTime)</td>
				<td>@Html.DisplayFor(modelItem => department.Description)</td>
				<td>@Html.DisplayFor(modelItem => department.ShortName)</td>
				<td>@Html.DisplayFor(modelItem => department.InstructorID)</td>
				<td>@Html.DisplayFor(modelItem => department.RowVersion)</td>
				<td>@Html.DisplayFor(modelItem => department.Administrator)</td>
				<td>@Html.DisplayFor(modelItem => department.Courses)</td>
			</tr>
			<td>
			<a asp-action="Details" class="btn btn-dark" asp-route-id="@department.Id">Details</a>
			<a asp-action="Delete" class="btn btn-primary" asp-route-id="@department.Id">Delete</a>
			<a asp-action="" class="btn btn-primary" asp-route-id="@department.Id">Edit"</a>
			<a asp-action="" class="btn btn-primary" asp-route-id=""></a>
			</td>
		}
	</tbody>
</table>